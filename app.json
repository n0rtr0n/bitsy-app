{
  "expo": {
    "name": "Bitsy Controller",
    "slug": "bitsy-app",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "permissions": []
      },
      "bundleIdentifier": "com.nathannorton.bitsyapp"
    },
    "description": "",
    "android": {
      "package": "com.nathannorton.bitsyapp",
      "permissions": []
    }
  }
}
